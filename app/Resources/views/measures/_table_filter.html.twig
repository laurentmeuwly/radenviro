<div class="header">
  {{ 'label.analyzed environments'|trans }}: {{ station.legends|first.legend.name }}<br/>
  {{ 'label.station'|trans }}:
   <select class="stations">
    {% for legendstation in station.legends|first.legend.stations %}
      <option value="{{ legendstation.station.id }}" 
              data-url="{{ url('measures', {'id': legendstation.station.id}) }}#measures-table"
              {% if legendstation.station.id==station.id %} selected="selected" {% endif %} >{{  legendstation.station.name }} ({{  legendstation.station.code }})</option>
    {% endfor %}
  </select>
  <br/>
  {{ 'label.isotope'|trans }}:
  
    <select class="nuclides nuclides-table">
         {% for legendnuclide in station.legends|first.legend.nuclides %}
        <option value="{{ legendnuclide.nuclide.id }}">{{ legendnuclide.nuclide.name }}</option>
      {% endfor %}
    </select>
</div>